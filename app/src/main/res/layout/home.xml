                            android:layout_weight="1">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:weightSum="1">

                                <com.budiyev.android.circularprogressbar.CircularProgressBar
                                    android:id="@+id/attprogress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:padding="10dp"
                                    app:animateProgress="true"
                                    app:backgroundStrokeColor="#ff3f51b5"
                                    app:backgroundStrokeWidth="2dp"
                                    app:drawBackgroundStroke="false"
                                    app:foregroundStrokeCap="butt"
                                    app:foregroundStrokeColor="#04F370"
                                    app:foregroundStrokeWidth="3dp"
                                    app:indeterminate="false"
                                    app:indeterminateMinimumAngle="45"
                                    app:indeterminateRotationAnimationDuration="1200"
                                    app:indeterminateSweepAnimationDuration="600"
                                    app:maximum="100"
                                    app:progress="0"
                                    app:progressAnimationDuration="100"
                                    app:startAngle="270" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:padding="10dp"
                                    android:text="Attempted"
                                    android:textAlignment="center"
                                    android:textColor="#02bbca" />


                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_margin="3dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="#fff"
                            android:elevation="4dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:weightSum="1">

                                <com.budiyev.android.circularprogressbar.CircularProgressBar
                                    android:id="@+id/wrongprogress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:padding="10dp"
                                    app:animateProgress="true"
                                    app:backgroundStrokeColor="#ff3f51b5"
                                    app:backgroundStrokeWidth="2dp"
                                    app:drawBackgroundStroke="false"
                                    app:foregroundStrokeCap="butt"
                                    app:foregroundStrokeColor="#ffff4081"
                                    app:foregroundStrokeWidth="3dp"
                                    app:indeterminate="false"
                                    app:indeterminateMinimumAngle="45"
                                    app:indeterminateRotationAnimationDuration="1200"
                                    app:indeterminateSweepAnimationDuration="600"
                                    app:maximum="100"
                                    app:progress="0"
                                    app:progressAnimationDuration="100"
                                    app:startAngle="270"
                                    />


                                <TextView
                                    android:gravity="center"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:padding="10dp"
                                    android:text="Wrong"
                                    android:textAlignment="center"
                                    android:textColor="#02bbca" />


                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</ScrollView>